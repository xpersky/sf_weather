<table class="table table-dark table-hover rounded">
  <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">City</th>
        <th scope="col">Country</th>
        <th scope="col">Temperature</th>
        <th scope="col">Created at</th>
    </tr>
  </thead>
  <tbody>
    {% for log in logs %}
    <tr>
        <th scope="row">{{ log.lp }}</th>
        <td>{{ log.city }}</td>
        <td>{{ log.country }}</td>
        <td>{{ log.temp }}</td>
        <td>{{ log.time|date('Y-m-d H:i') }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>